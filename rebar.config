{deps, [
  {json,
   {git, "git://github.com/talentdeficit/json.git",  {branch, "master"}}},
  {elli,        "1.0.5"},
  {elli_cookie, "0.1.7"}
]}.

{plugins, [
  {'lfe-compile',
   {git, "git://github.com/lfe-rebar3/compile.git",
    {tag, "0.5.0"}}}
]}.

{provider_hooks, [
  {post, [{compile, {lfe, compile}}]}
]}.

{project_plugins, [
  {lodox, {git, "git://github.com/lfe-rebar3/lodox.git", {tag, "0.15.1"}}}
]}.

{lodox, [
  {apps, [
    {elli_cloudfront, [
      {'source-uri',
       "https://github.com/elli-lib/elli_cloudfront"
       "/blob/{version}/{filepath}#L{line}"}
    ]}
  ]}
]}.
