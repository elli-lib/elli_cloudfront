{deps, [
  {lfe,  {git, "git://github.com/rvirding/lfe.git",       {tag,    "1.0.1"}}},
  {clj,  {git, "git://github.com/lfex/clj.git",           {tag,    "0.4.0"}}},
  {json, {git, "git://github.com/talentdeficit/json.git", {branch, "master"}}},
  {elli, "1.0.5"},
  {elli_cookie,
   {git, "git://github.com/yurrriq/elli_cookie.git",      {branch, "master"}}}
]}.

{plugins, [
  {'lfe-compile',
   {git, "git://github.com/lfe-rebar3/compile.git",       {tag,    "0.3.0"}}}
]}.

{provider_hooks, [{post, [{compile, {lfe, compile}}]}]}.

{project_plugins, [
  {lodox, {git, "git://github.com/lfe-rebar3/lodox.git",  {tag,    "0.12.12"}}}
]}.

{lodox, [
  {apps, [
    {elli_cloudfront, [
      {'source-uri',
       "https://github.com/yurrriq/elli_cloudfront"
       "/blob/{version}/{filepath}#L{line}"}
    ]}
  ]}
]}.
