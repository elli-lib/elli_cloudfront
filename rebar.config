{deps, [
  {clj,  "0.5.0"},
  {json, {git, "git://github.com/talentdeficit/json.git", {branch, "master"}}},
  {elli, "1.0.5"},
  {elli_cookie,
   {git, "git://github.com/yurrriq/elli_cookie.git",      {branch, "master"}}}
]}.

{plugins, [rebar3_lfe_compile]}.

{provider_hooks, [{post, [{compile, {lfe, compile}}]}]}.

{project_plugins, [lodox]}.

{lodox, [
  {apps, [
    {elli_cloudfront, [
      {'source-uri',
       "https://github.com/yurrriq/elli_cloudfront"
       "/blob/{version}/{filepath}#L{line}"}
    ]}
  ]}
]}.
